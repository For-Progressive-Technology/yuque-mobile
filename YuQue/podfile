  use_frameworks!
  
  pod 'SnapKit'
  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod "Kingfisher"
  pod "R.swift"
  pod 'IQKeyboardManager'
  pod 'Toast-Swift', '~> 5.0.1'
  
  #  Debug 中使用
  pod 'DoraemonKit/Core', :git => "https://gitee.com/yixiangboy/DoraemonKit.git", :configurations => ['Debug']
  pod 'DoraemonKit/WithLogger', :git => "https://gitee.com/yixiangboy/DoraemonKit.git", :configurations => ['Debug']
  pod 'DoraemonKit/WithGPS', :git => "https://gitee.com/yixiangboy/DoraemonKit.git", :configurations => ['Debug']
  pod 'DoraemonKit/WithLoad', :git => "https://gitee.com/yixiangboy/DoraemonKit.git", :configurations => ['Debug']
  pod 'DoraemonKit/WithDatabase', :git => "https://gitee.com/yixiangboy/DoraemonKit.git", :configurations => ['Debug']
  pod 'DoraemonKit/WithMLeaksFinder', :git => "https://gitee.com/yixiangboy/DoraemonKit.git", :configurations => ['Debug']
end
